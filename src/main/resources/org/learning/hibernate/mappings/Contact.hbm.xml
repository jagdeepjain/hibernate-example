<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
 "-//Hibernate/Hibernate Mapping DTD//EN"
 "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>

	<class name="org.learning.hibernate.Contact" table="CONTACT">
		<meta attribute="class-description">
			Employee details hibernate mapping file.
		</meta>
		<id name="id" type="int" column="ID">
			<generator class="native" />
		</id>
		<property name="fullName" column="FULLNAME" type="java.lang.String" />
		<property name="mobileNumber" column="MOBILENUMBER" type="java.lang.String" />
		<property name="emailID" column="EMAILID" type="java.lang.String" />
	</class>
	
</hibernate-mapping>